<body>

    <!--Home-->
    <div id="home">
        <!-- Header -->
        <div class="navbar">
        <ul>
            <li><a href="#home">Home</a></li>
            <li><a href="#about">About</a></li>
            <li><a href="#services">Services</a></li>
            <li><a href="#education">Education</a></li>
            <li><a>Pages</a>
                <ul>
                    <li><a href="#projects">Projects</a></li>
                    <li><a href="#images">Images</a></li>
                    <li><a href="#testimonials">Testimonials</a></li>
                </ul>
            </li>
            <li><a href="#contact">Contact</a></li>
        </ul>

        </div>

        <!-- Content -->

        <div class="mydetails">
            <h1><%= home.name %></h1>
            <div class="designation"><%=home.designation%></div>

        </div>





    </div>

    <!--About-->
    <div id="about">
        <div class="aboutouter">
            <h1 class="aboutheading"><i class="fas fa-male"></i> <%= about.title %> <i class="fas fa-male"></i></h1>
            <div class="abconouter">
                <div class="aboutcontent box2">
                    <h3 class="abouttitle"><%= about.abouttitle %></h3>
                    <p class="aboutdesc"><%= about.aboutdesc %><span class="dots">...</span><span class="moredesc"><%= about.moredescription %></span></p>
                    <a class="readmore"> Read More </a>
                </div>

                <div class="aboutimage box2">
                    <img src="../public/img1.png">
                </div>

                <div class="aboutbars box2">
                    <% about.skills.forEach(function(skill){  %>
                        <div class="progdetail">
                            <div class="skilldetail">
                                <p class="skillname"><%= skill.name %><span><%= skill.percentage %></span></p>
                            </div>
                            <!--<div class="w3-red" style="height:24px;width:%"></div>-->
                            <div class="meter red"><span style="width: <%= skill.percentage %>%"></span></div>
                        </div>
                    <%  }) %>
                </div>

            </div>
        </div>
    </div>

    <!--Services-->
    <div id="services">
        <div class="serviceheader"><%= services.title %></div>
        <div class="servicelist">
            <% services.details.forEach(function(service){ %>
            <div class="serviceitems">
                <div class="serviceitemname"><i class="<%= service.icon %>"></i> <%= service.name %></div>
                <div class="serviceiteminfo"><%= service.description %></div>
            </div>
            <% }) %>
        </div>
    </div>


    <!--Education-->
    <div id = 'education' class = 'education'>
        <div class = 'educationtop'>
            <div class ='educationtoptext'>
                Education
            </div>
        </div>
        <div class = 'educationbottom'>
            <% for(let i = 0; i< education.length;i++) { %>
                <% if((i%2)===0) { %>
                    <div class= 'educationleft'>
                        <h3><%= education[i].level %></h3>
                        <div class = 'eltext'>
                            <h4>Year : <%= education[i].year %></h4>
                            <%if(i!==2){%><h4>Specialisation : <%= education[i].specialisation %> </h4><% } %>
                            <h4>School/College : <%= education[i].school %></h4>
                            <% if(i!==0){ %><h4>Board/University : <%= education[i].board %></h4><%}%>
                        </div>
                    </div>
                <% } else {%>
                    <div class ='educationright'>
                        <div class = 'ertext'>
                            <h4>Year : <%= education[i].year %></h4>
                            <h4>Specialisation : <%= education[i].specialisation %></h4>
                            <h4>School/College : <%= education[i].school %></h4>
                            <h4>Board/University : <%= education[i].board %></h4>
                        </div>
                        <h3><%= education[i].level %></h3>

                    </div>
                <%}%>
            <% }%>
        </div>
    </div>

    <!--Projects-->
    <div id="projects">
        <h1 class="projectheading">Projects</h1>
        <div class="allprojects">
            <div class="projectoverview">
                <h2 class="overviewtitle"><%= project.overview.title %></h2>
                <p class="overviewdetails"><%= project.overview.details %></p>
            </div>
            <div class="project3 borderstyle">
                <h5 class="project3date"><%= project.list[2].date %></h5>
                <h4 class="project3name"><%= project.list[2].title %></h4>
                <p class="project3detail"><%= project.list[2].detail %></p>
            </div>

            <div class="project1 borderstyle">
                <h5 class="project1date"><%= project.list[0].date %></h5>
                <h4 class="project1name"><%= project.list[0].title %></h4>
                <p class="project1detail"><%= project.list[0].detail %></p>
            </div>

            <div class="project2 borderstyle">
                <h5 class="project2date"><%= project.list[1].date %></h5>
                <h4 class="project2name"><%= project.list[1].title %></h4>
                <p class="project2detail"><%= project.list[1].detail %></p>
            </div>

        </div>
    </div>

    <!-- Images -->
    <div id="images">
        <div class="imgheading">
            Images
        </div>
        <div class="imagegrid">
            <div class="line1">
                <img src="<%= images[0] %>" class="img1">
                <img src="<%= images[1] %>" class="img2">
                <img src="<%= images[2] %>" class="img3">
            </div>
            <div class="line2">
                <img src="<%= images[3] %>" class="img4">
                <img src="<%= images[4] %>" class="img5">
                <img src="<%= images[5] %>" class="img6">
            </div>
        </div>
    </div>

