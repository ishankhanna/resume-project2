    <!-- Testimonials 2 -->
    <div class= "container6" id="testimonials">
        <div class="testheading">
            <h1>Testimonials</h1>
        </div>
        <!-- Slideshow container -->
        <div class="slideshow-container">
            <% for(let i = 0 ; i< testimonials.image_loc.length ; i++) { %>
                <div class="grid_container7">
                    <div class="grid_container7_ele">
                        <div class="grid_container7_ele_left fade">
                            <img src="<%- testimonials.image_loc[i]%>" no-repeat alt="Image">
                        </div>
                        <div class="grid_container7_ele_right fade">
                            <p>
                                <%= testimonials.content[i] %>
                            </p>
                        </div>
                    </div>
                </div>
            <% } %>
            <br>
        </div>
    </div>




    <!--Contact-->
        <div id = 'contact' class='contact'>
            <div class = 'contacttop'>
                <div class ='contacttoptext'>
                    <%= contact.title %>
                </div>
            </div>
            <div class='contactbottom'>
                <div class='cbleft'>
                    <h3>Address Info</h3>
                    <% contact.content.forEach(function(type) {%>
                        <div class= 'contacthead'>
                            <i class="<%- type.icon %>"></i>
                            <h4><%= type.name %></h4>
                        </div>
                        <div class='contactvalues'>
                            <% type.content.forEach(function(list){ %>
                                <p><%= list %></p>
                            <% }) %>
                        </div>
                    <% }) %>
                </div>
                <div class='cbright'>
                    <h3>Get In Touch</h3>
                    <div id="postform">
                        <div class='form-name form-elem '>
                            <label><i class="fas fa-user" id="form-name" aria-hidden="true"></i> Name</label>
                            <input type="text" name="Name" id="Name-Input" value="Name" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Name';}" required="">
                        </div>
                        <div class = 'form-email form-elem'>
                            <label><i class="fas fa-envelope" id="form-email" aria-hidden="true"></i>Email</label>
                            <input type="email" name="Email" id="Email-Input" value="Email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Email';}" required="">
                        </div>
                        <div class = 'form-phone form-elem'>
                            <label><i class="fas fa-phone" id="form-phone" aria-hidden="true"></i>Phone</label>
                            <input type="tel" name="Phone" id="Phone-Input" value="Phone" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Phone';}" required="">
                        </div>
                        <div class='form-message form-elem'>
                            <label><i class="far fa-edit" id="form-message" aria-hidden="true"></i>Message</label>
                            <textarea name="Message" id="Message-Input" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Message...';}" required="">Message...</textarea>
                        </div>
                        <div class='form-submit form-elem'>
                            <button id="submit-form">Submit</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>




    <!-- footer -->
    <footer class="py-5">
        <div class="container">
            <div class="agileinfo-grids">
                <div class="agile-grid-left agile-grid-right text-center">
                    <div class="social mb-4">
                        <ul>
                            <% footer.sites.forEach(function(site){ %>
                                <li><a href="<%= site.link %>"<% if(site.name === "resume"){ %>  <%= "download" %> <%}%>><i class="<%= site.icon %>"></i></a></li>
                            <% }) %>
